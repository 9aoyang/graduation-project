{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": 0,
        "jsx-a11y/anchor-is-valid":0,
        "prefer-destructuring": ["error", {
            "array": false,
            "object": false
          }, {
            "enforceForRenamedProperties": false
          }]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "parser": "babel-eslint"
}
